{
  "expo": {
    "name": "ColocMbds",
    "slug": "ColocMbds",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/LogoApp.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/LogoApp.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/LogoApp.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/LogoApp.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "L'application nécessite l'accès à vos photos pour télécharger des images de votre bien."
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "L'application nécessite l'accès à votre localisation pour trouver votre bien."
        }
      ]
    ],
    "newArchEnabled": true
  }
}